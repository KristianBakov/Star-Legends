.background-box {
	width: 100%;
	height: 100%;
	background-color: #839EC9;
}

.title-label {
	font-size: 88px;
	color: #FFFFFF;
	-unity-font-definition: url("/Assets/Fonts/steelfish-bold.asset");
	padding-left: 12px;
}

.agent-select-list-view {
	width: 400px;
	height: 100%;
	background-color: #839EC9;
	flex-direction: column;
}

.agent-select-button {
	white-space: normal;
	margin-bottom: 12px;
	min-width: 80px;
	padding: 24px 12px;
	-unity-font-definition: url("/Assets/Fonts/DINN-Regular.asset");
	color: #ffffff;
	background-color: #6DB09C;
	font-size: 24px;
	color: #FFFFFF;
	-unity-text-align: middle-center;
	border-radius: 0px;
	border-width: 1px;
	border-color: #FFFFFF;
}

.agent-select-button:hover {
	background-color: #061621;
}
